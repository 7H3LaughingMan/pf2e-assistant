{
    "id": "pf2e-assistant",
    "title": "PF2e Assistant",
    "description": "Your personal assistant for your Pathfinder 2nd Edition games, helps automate various things that aren't automated by the game system.",
    "version": "7.8.0",
    "authors": [
        {
            "name": "7H3LaughingMan",
            "discord": "7h3laughingman",
            "url": "https://github.com/7H3LaughingMan",
            "flags": {}
        }
    ],
    "socket": true,
    "compatibility": {
        "minimum": "13.348",
        "verified": "13.351",
        "maximum": "13"
    },
    "esmodules": [
        "dist/pf2e-assistant.js"
    ],
    "relationships": {
        "requires": [
            {
                "id": "socketlib",
                "type": "module",
                "manifest": "https://github.com/farling42/foundryvtt-socketlib/releases/latest/download/module.json"
            },
            {
                "id": "lib-wrapper",
                "type": "module",
                "manifest": "https://github.com/ruipin/fvtt-lib-wrapper/releases/latest/download/module.json"
            }
        ],
        "systems": [
            {
                "id": "pf2e",
                "type": "system",
                "manifest": "https://raw.githubusercontent.com/foundryvtt/pf2e/v13-dev/static/system.json",
                "compatibility": {
                    "minimum": "7.8.0"
                }
            }
        ]
    },
    "packs": [
        {
            "name": "pf2e-macros",
            "label": "PF2e Assistant Macros",
            "path": "packs/pf2e-macros",
            "type": "Macro",
            "banner": "systems/pf2e/assets/compendium-banner/orange.webp",
            "system": "pf2e",
            "ownership": {
                "PLAYER": "OBSERVER",
                "ASSISTANT": "OWNER"
            }
        }
    ],
    "packFolders": [
        {
            "name": "Macros",
            "sorting": "m",
            "color": "#181818",
            "packs": [
                "pf2e-macros"
            ]
        }
    ],
    "url": "https://github.com/7H3LaughingMan/pf2e-assistant",
    "manifest": "",
    "download": "",
    "license": "https://github.com/7H3LaughingMan/pf2e-assistant/blob/main/LICENSE.md",
    "readme": "https://github.com/7H3LaughingMan/pf2e-assistant/blob/main/README.md",
    "bugs": "https://github.com/7H3LaughingMan/pf2e-assistant/issues",
    "changelog": "https://github.com/7H3LaughingMan/pf2e-assistant/blob/main/CHANGELOG.md"
}